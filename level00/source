#include <stdio.h>

int main(void) {
	int		pas;

	puts("***********************************");
	puts("* \t     -Level00 -\t\t  *");
	puts("***********************************");
	printf("Password:");
	scanf("%d", &pas);

	if (pas == 5276) {
		puts("\nAuthenticated!");
		system("/bin/sh");

		return (0);
	}

	puts("\nInvalid Password!");
	return (1);
}
